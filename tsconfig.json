{
  "compilerOptions": {
    "target": "ES6",
    "strict": true,
    "module": "ES6",
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "dist",
  ],
}
